{
    "name": "reactdearimgui",
    "version-string": "1.0.0",
    "vcpkg-configuration": {
        "overlay-ports": [
            "./vcpkg-overlay-ports/imgui"
        ]
    },
    "dependencies": [
        "nlohmann-json",
        {
            "name": "imgui",
            "features": [
                "glfw-binding",
                "opengl3-binding",
                "wgpu-binding",
                "wchar32"
            ]
        },
        "implot",
        "opengl-registry"
    ]
}